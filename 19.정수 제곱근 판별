using System;

public class Solution {
    public long solution(long n) {
        long answer = 0;
        int result;
        
        double a = Math.Sqrt(n);
        long b = (long)a;
        
        if (int.TryParse(a.ToString(), out result))
        {
            answer = (b+1)*(b+1);
        }
        else
        {
            answer = -1;
        }
        
        return answer;
    }
}
