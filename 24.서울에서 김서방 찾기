using System;
using System.Text;

public class Solution {
    public string solution(string[] seoul) {
        string result = "";
        
        for (int i = 0; i < seoul.Length; i++)
        {
            if (seoul[i] == "Kim")
            {
                result = i.ToString();
                break;
            }
        }
        
        string answer = "김서방은 " + result + "에 있다";
        
        return answer;
    }
}
