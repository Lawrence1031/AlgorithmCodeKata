using System;
using System.Linq;

public class Solution {
    public string[] solution(string[] strings, int n) {
        string[] answer = new string[] {};
        
        // 리스트 strings에서 n번째 글자를 기준으로 오름차순 정리
        // 원소들은 소문자 알파벳으로
        
        // for 문으로 각 리스트에서 n번째 글자를 뽑아오기
        // 그 n번째 글자들로 정리하기
        // 정리한 순서대로 배열을 만들기
        
        // OrderBy를 쓰면 될거 같음
        
        string[] newWord = strings;
        
        answer = newWord.OrderBy(s => s[n]).ThenBy(s => s).ToArray();
        
        return answer;
    }
}
