using System;

public class Solution {
    public bool solution(string s) {
        
        if (s.Length == 4 || s.Length == 6)
        {
            foreach (char c in s)
            {
                if (!char.IsDigit(c))
                {
                    return false;
                }
            }
            return true;
        }
        return false;
        
        
//         int a = 0;
//         bool result = int.TryParse(s, out a);
        
//         // 문자열 s의 길이가 4 혹은 6인지 확인하기
//         // 아니면 False, 맞으면 다음 단계로
//         // 문자열이 숫자로만 구성되어 있는지 확인하기
//         // 아니면 False, 맞으면 True 리턴
        
//         if (s.Length == 4 || s.Length == 6)
//         {
//             if (a != 0)
//                 answer = true;
//             else
//                 answer = false;
//         }
//         else
//             answer = false;
//         0000이나 000000이 false가 나와서 다른 방법 찾기
    }
}
